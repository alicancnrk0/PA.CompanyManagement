{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "expenseRoute": {
        "ClusterId": "accounting-meta-expense-cluster",
        "Match": {
          "Path": "api/mt/ex/{**mtex}"
        },
        "Transforms": [
          { "PathPattern": "{**mtex}" }
        ]
      },
      "incomeRoute": {
        "ClusterId": "accounting-meta-income-cluster",
        "Match": {
          "Path": "api/mt/ic/{**mtic}"
        },
        "Transforms": [
          { "PathPattern": "{**mtic}" }
        ]
      },
      "expenseTypeRoute": {
        "ClusterId": "accounting-type-expense-cluster",
        "Match": {
          "Path": "api/ty/ex/{**tyex}"
        },
        "Transforms": [
          { "PathPattern": "{**tyex}" }
        ]
      },
      "incomeTypeRoute": {
        "ClusterId": "accounting-type-income-cluster",
        "Match": {
          "Path": "api/ty/ic/{**tyic}"
        },
        "Transforms": [
          { "PathPattern": "{**tyic}" }
        ]
      }
    },
    "Clusters": {
      "accounting-meta-expense-cluster": {
        "Destinations": {
          "default": {
            "Address": "https://localhost:7151/api/expense"
          }
        }
      },
      "accounting-meta-income-cluster": {
        "Destinations": {
          "default": {
            "Address": "https://localhost:7151/api/income"
          }
        }
      },
      "accounting-type-expense-cluster": {
        "Destinations": {
          "default": {
            "Address": "https://localhost:7151/api/expense-type"
          }
        }
      },
      "accounting-type-income-cluster": {
        "Destinations": {
          "default": {
            "Address": "https://localhost:7151/api/income-type"
          }
        }
      }
    }
  }
}
